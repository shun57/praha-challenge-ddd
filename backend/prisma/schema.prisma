datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
}

model Participant {
  id                String   @id
  name              String
  email             String   @unique
  enrollmentStatus  String
}

model Team {
  id                String   @id
  name              String
}

model Pair {
  id                String   @id
  name              String
}
